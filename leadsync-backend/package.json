{
  "name": "leadsync-backend",
  "version": "1.0.0",
  "description": "LeadSync Backend API",
  "main": "dist/server.js",
  "type": "commonjs",
  "scripts": {
    "dev": "ts-node-dev --respawn --transpile-only src/server.ts",
    "ngrok": "ngrok http 4000",
    "dev:all": "concurrently \"npm run dev\" \"npm run ngrok\"",
    "seed": "ts-node -P tsconfig.prisma.json prisma/seed.ts",
    "build": "tsc",
    "start": "node -r dotenv/config dist/server.js"
  },
  "dependencies": {
    "@prisma/client": "^6.6.0",
    "axios": "^1.13.5",
    "bcryptjs": "^3.0.3",
    "cors": "^2.8.6",
    "dotenv": "^17.2.4",
    "express": "^4.22.1",
    "jsonwebtoken": "^9.0.3",
    "node-fetch": "^2.7.0"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.6",
    "@types/cors": "^2.8.19",
    "@types/express": "^4.17.25",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/node": "^20.19.33",
    "@types/node-fetch": "^2.6.13",
    "concurrently": "^9.2.0",
    "prisma": "^6.6.0",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.9.3"
  }
}
